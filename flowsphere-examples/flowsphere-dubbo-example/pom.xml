<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>io.github.zshun</groupId>
        <artifactId>flowsphere-examples</artifactId>
        <version>0.0.1-SNAPSHOT</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <artifactId>flowsphere-dubbo-example</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>flowsphere-dubbo-example</name>
    <packaging>pom</packaging>
    <description>flowsphere-dubbo-example</description>
    <properties>
        <java.version>1.8</java.version>
    </properties>

    <modules>
        <module>flowsphere-dubbo-api-example</module>
        <module>flowsphere-dubbo-service-a-example</module>
        <module>flowsphere-dubbo-service-a1-example</module>
        <module>flowsphere-dubbo-service-b-example</module>
        <module>flowsphere-dubbo-service-b1-example</module>
        <module>flowsphere-dubbo-service-c-example</module>
        <module>flowsphere-dubbo-service-c1-example</module>
        <module>flowsphere-web-example</module>
    </modules>


    <dependencyManagement>

        <dependencies>

            <dependency>
                <groupId>com.alibaba.cloud</groupId>
                <artifactId>spring-cloud-alibaba-dependencies</artifactId>
                <version>2.2.7.RELEASE</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>

            <dependency>
                <groupId>com.alibaba.spring</groupId>
                <artifactId>spring-context-support</artifactId>
                <version>1.0.11</version>
            </dependency>

        </dependencies>

    </dependencyManagement>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

</project>
